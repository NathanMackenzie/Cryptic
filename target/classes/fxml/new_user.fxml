<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>

<VBox fx:id="root" alignment="CENTER" maxHeight="350.0" maxWidth="250.0" minHeight="350.0" minWidth="250.0" prefHeight="350.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cryptic.controller.NewUserController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Add User">
         <font>
            <Font name="Avenir Medium Oblique" size="36.0" />
         </font>
         <VBox.margin>
            <Insets bottom="40.0" />
         </VBox.margin></Text>
      <TextField fx:id="userNameField" promptText="User Name">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin></TextField>
      <PasswordField fx:id="passwordField" promptText="Password">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin></PasswordField>
      <PasswordField fx:id="verifyPasswordField" promptText="Verify Password">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" />
         </VBox.margin></PasswordField>
      <Text fx:id="warningLabel" strokeType="OUTSIDE" strokeWidth="0.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </Text>
      <Button mnemonicParsing="false" onAction="#createUser" text="Add User" />
   </children>
</VBox>
